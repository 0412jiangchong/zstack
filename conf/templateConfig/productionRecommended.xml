<?xml version="1.0" encoding="UTF-8"?>
<templateConfig xmlns="http://zstack.org/schema/zstack">
    <config>
        <templateName>ProductionRecommended</templateName>
        <category>kvm</category>
        <name>reservedMemory</name>
        <defaultValue>16G</defaultValue>
    </config>

    <config>
        <templateName>ProductionRecommended</templateName>
        <category>mevoco</category>
        <name>overProvisioning.primaryStorage</name>
        <defaultValue>1.0</defaultValue>
    </config>

    <config>
        <templateName>ProductionRecommended</templateName>
        <category>mevoco</category>
        <name>overProvisioning.memory</name>
        <defaultValue>1.0</defaultValue>
    </config>

    <config>
        <templateName>ProductionRecommended</templateName>
        <category>localStoragePrimaryStorage</category>
        <name>liveMigrationWithStorage.allow</name>
        <defaultValue>true</defaultValue>
        <type>java.lang.Boolean</type>
    </config>

    <config>
        <templateName>ProductionRecommended</templateName>
        <category>ha</category>
        <name>enable</name>
        <type>java.lang.Boolean</type>
        <defaultValue>true</defaultValue>
    </config>

    <config>
        <templateName>ProductionRecommended</templateName>
        <category>host</category>
        <name>cpu.overProvisioning.ratio</name>
        <defaultValue>4</defaultValue>
        <type>java.lang.Integer</type>
    </config>

    <config>
        <templateName>ProductionRecommended</templateName>
        <category>mevoco</category>
        <name>threshold.primaryStorage.physicalCapacity</name>
        <defaultValue>0.8</defaultValue>
        <type>java.lang.Double</type>
    </config>

    <config>
        <templateName>ProductionRecommended</templateName>
        <category>vm</category>
        <name>cleanTraffic</name>
        <defaultValue>true</defaultValue>
        <type>java.lang.Boolean</type>
    </config>

    <config>
        <templateName>ProductionRecommended</templateName>
        <category>ha</category>
        <name>allow.slibing.cross.clusters</name>
        <defaultValue>true</defaultValue>
        <type>java.lang.Boolean</type>
    </config>

    <config>
        <templateName>ProductionRecommended</templateName>
        <category>primaryStorage</category>
        <name>reservedCapacity</name>
        <defaultValue>200G</defaultValue>
    </config>

    <config>
        <templateName>ProductionRecommended</templateName>
        <category>backupStorage</category>
        <name>reservedCapacity</name>
        <defaultValue>200G</defaultValue>
    </config>

    <config>
        <templateName>ProductionRecommended</templateName>
        <category>zwatch</category>
        <name>scrape.interval</name>
        <type>java.lang.Integer</type>
        <defaultValue>10</defaultValue>
    </config>

    <config>
        <templateName>ProductionRecommended</templateName>
        <category>prometheus</category>
        <name>storage.local.retention</name>
        <type>java.lang.Integer</type>
        <defaultValue>3</defaultValue>
    </config>

    <config>
        <templateName>ProductionRecommended</templateName>
        <category>quota</category>
        <name>snapshot.volume.num</name>
        <type>java.lang.Integer</type>
        <defaultValue>5</defaultValue>
    </config>

    <config>
        <templateName>ProductionRecommended</templateName>
        <category>encrypt</category>
        <name>enable.password.encrypt</name>
        <type>java.lang.Boolean</type>
        <defaultValue>true</defaultValue>
    </config>


</templateConfig>
<?xml version="1.0" encoding="UTF-8"?>
<templateConfig xmlns="http://zstack.org/schema/zstack">
    <config>
        <templateName>VmPerformanceOptimised</templateName>
        <category>kvm</category>
        <name>vm.cpuMode</name>
        <defaultValue>host-passthrough</defaultValue>
    </config>

    <config>
        <templateName>VmPerformanceOptimised</templateName>
        <category>mevoco</category>
        <name>aio.native</name>
        <defaultValue>true</defaultValue>
        <type>java.lang.Boolean</type>>
    </config>

    <config>
        <templateName>VmPerformanceOptimised</templateName>
        <category>sharedblock</category>
        <name>qcow2.allocation</name>
        <defaultValue>falloc</defaultValue>
    </config>

    <config>
        <templateName>VmPerformanceOptimised</templateName>
        <category>sharedblock</category>
        <name>qcow2.cluster.size</name>
        <defaultValue>2097152</defaultValue>
        <type>java.lang.Integer</type>
    </config>

    <config>
        <templateName>VmPerformanceOptimised</templateName>
        <name>emulateHyperV</name>
        <category>vm</category>
        <defaultValue>true</defaultValue>
        <type>java.lang.Boolean</type>
    </config>

</templateConfig>